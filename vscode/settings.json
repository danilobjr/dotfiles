{
  "editor.minimap.enabled": false,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/**.js": {
      "when": "$(basename).tsx"
    }
  },
  "keyboard.dispatch": "keyCode",
  "vim.smartRelativeLine": true,
  "vim.leader": "<space>",
  "vim.statusBarColorControl": true,
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["h"]
    },
    {
      "before": ["k"],
      "after": ["j"]
    },
    {
      "before": ["l"],
      "after": ["k"]
    },
    {
      "before": [";"],
      "after": ["l"]
    },
    {
      "before": ["<leader>", "w", "w"],
      "commands": ["workbench.action.files.save"]
    },
    {
      "before": ["<leader>", "w", "a"],
      "commands": ["workbench.action.files.saveAll"]
    },
    {
      "before": ["<leader>", "q", "q"],
      "commands": ["workbench.action.closeActiveEditor"]
    },
    {
      "before": ["<leader>", "q", "a"],
      "commands": ["workbench.action.closeAllEditors"]
    },
    {
      "before": ["<leader>", "o"],
      "after": ["o", "<esc>"]
    },
    {
      "before": ["<leader>", "O"],
      "after": ["O", "<esc>"]
    },
    {
      "before": ["<leader>", "d"],
      "after": ["S", "<esc>"]
    },
    {
      "before": ["<leader>", "s"],
      "after": [":", "%", "s", "/", "<c-r>", "<c-w>", "/", "/", "g", "<left>", "<left>"]
    },
    {
      "before": ["<leader>", "S"],
      "after": [":", "%", "s", "/", "<c-r>", "<c-w>", "/", "<c-r>", "<c-w>", "/", "g", "<left>", "<left>"]
    }
  ],
  "vim.visualModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["h"]
    },
    {
      "before": ["k"],
      "after": ["j"]
    },
    {
      "before": ["l"],
      "after": ["k"]
    },
    {
      "before": [";"],
      "after": ["l"]
    }
  ],
  "vim.operatorPendingModeKeyBindingsNonRecursive": [
    {
      "before": ["j"],
      "after": ["h"]
    },
    {
      "before": ["k"],
      "after": ["j"]
    },
    {
      "before": ["l"],
      "after": ["k"]
    },
    {
      "before": [";"],
      "after": ["l"]
    }
  ],
  "vim.statusBarColors.normal": [
    "#3e7aca",
    "#FFFFFF"
  ],
  "vim.statusBarColors.insert": [
    "#679436",
    "#FFFFFF"
  ],
  "vim.statusBarColors.visual": [
    "#D5DFE5",
    "#3C4859B3"
  ],
  "vim.statusBarColors.visualline": [
    "#D5DFE5",
    "#3C4859B3"
  ],
  "vim.statusBarColors.visualblock": [
    "#D5DFE5",
    "#3C4859"
  ],
  "vim.statusBarColors.replace": [
    "#D05353",
    "#FFFFFF"
  ],
  "window.menuBarVisibility": "toggle",
  "window.openFilesInNewWindow": "off",
  "window.zoomLevel": 0,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorCustomizations": {
    "statusBar.background": "#679436",
    "statusBar.noFolderBackground": "#679436",
    "statusBar.debuggingBackground": "#679436",
    "statusBar.foreground": "#FFFFFF"
  },
  "typescript.updateImportsOnFileMove.enabled": "never",
  "banner-comments-plus.favorites": [
    "ANSI Shadow"
  ],
  "todo-tree.tree.showScanModeButton": false,
  "editor.fontFamily": "'JetBrains Mono', 'Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.fontLigatures": true
}