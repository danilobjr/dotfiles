{
  "editor.minimap.enabled": false,
  "editor.snippetSuggestions": "top",
  "editor.tabCompletion": "on",
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "emmet.triggerExpansionOnTab": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "**/*.js": {
      "when": "$(basename).ts"
    },
    "**/**.js": {
      "when": "$(basename).tsx"
    }
  },
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.normal": "#3e7aca",
  "vim.statusBarColors.insert": "#679436",
  "vim.statusBarColors.visual": [
    "#D5DFE5",
    "#3C4859B3"
  ],
  "vim.statusBarColors.visualline": [
    "#D5DFE5",
    "#3C4859B3"
  ],
  "vim.statusBarColors.visualblock": [
    "#D5DFE5",
    "#3C4859"
  ],
  "vim.statusBarColors.replace": "#D05353",
  "window.menuBarVisibility": "toggle",
  "window.openFilesInNewWindow": "off",
  "window.zoomLevel": 0,
  "workbench.iconTheme": "eq-material-theme-icons",
  "workbench.colorCustomizations": {
    "statusBar.background": "#3e7aca",
    "statusBar.noFolderBackground": "#3e7aca",
    "statusBar.debuggingBackground": "#3e7aca"
  },
  "typescript.updateImportsOnFileMove.enabled": "never",
  "banner-comments-plus.favorites": [
    "ANSI Shadow"
  ]
}